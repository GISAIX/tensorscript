{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {"all": true}
    },
    {
      "name": "esdoc-standard-plugin",
      "option":{
        "typeInference": {
          "enable": false
        },
        "lint": {
          "enable": false
        },
        "manual": {
          "index": "./manual/index.md",
          "asset": "./manual/asset",
          "files": [
            "./manual/overview.md",
            "./manual/installation.md",
            "./manual/usage.md",
            "./manual/tutorial.md",
            "./manual/configuration.md",
            "./manual/example.md",
            "./CHANGELOG.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-publish-markdown-plugin",
      "option": {
        "filename":"./API.md"
      }
    }
  ],
  "includes": ["\\.mjs$"]
}
